{
  "name": "JK Shortcuts Navigator",
  "version": "1.2.0.3",
  "manifest_version": 2,
  "description": "Use keyboard shortcuts to navigate with the most popular websites.",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "options_page":"options.html", 
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["lib/jquery.min.js", "keymaster.min.js", "lib/underscore-min.js", "script.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "page_action": {
    "default_icon": "icon/icon16.png",
    "default_title": "JK Shortcuts Navigator",
    "default_popup": "page_action.html"
  },
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png",
    "256": "icon/icon256.png"
  }
}

